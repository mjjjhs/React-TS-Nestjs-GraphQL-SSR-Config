{
  "name": "nest-next-template",
  "version": "1.0.0",
  "description": "LQT Frontend project configuration template",
  "author": "lqt-front",
  "scripts": {
    "dev": "nodemon src/main.ts",
    "build:clean": "rimraf .next",
    "build:ui": "next build",
    "build:server": "tsc --project tsconfig.server.json && babel .next/production-server -d .next/production-server --extensions \\\".js\\\"",
    "build": "npm run build:clean && npm run build:ui && npm run build:server",
    "generate": "TS_NODE_PROJECT=tsconfig.server.json ts-node src/generate-typings",
    "start": "cross-env NODE_ENV=production node .next/production-server/src/main.js",
    "development": "npm run storybook-development && pm2-runtime start ecosystem.config.js --env development",
    "qa": "pm2-runtime start ecosystem.config.js --env qa",
    "sandbox": "pm2-runtime start ecosystem.config.js --env sandbox",
    "staging": "pm2-runtime start ecosystem.config.js --env staging",
    "production": "pm2-runtime start ecosystem.config.js --env production",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "storybook-development": "pm2 start storybookEcosystem.config.js --env development",
    "storybook-qa": "pm2 start storybookEcosystem.config.js --env qa",
    "test": "jest --verbose --coverage",
    "test:info": "open coverage/lcov-report/index.html",
    "test:report": "open coverage/test-report.html",
    "eb:dev": "npm run development",
    "eb:qa": "npm run qa",
    "eb:sandbox": "npm run sandbox",
    "eb:stage": "npm run staging",
    "eb:prod": "npm run production"
  },
